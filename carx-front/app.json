{
  "expo": {
    "name": "carx",
    "slug": "carx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconX.png",
    "splash": {
      "image": "./assets/iconX.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.gofar.carx",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.gofar.carx",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconX.png",
        "backgroundColor": "#FFFFFF"
      }  
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }
}
